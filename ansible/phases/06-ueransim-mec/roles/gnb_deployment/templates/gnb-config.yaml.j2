mcc: '{{ ueransim_topology.defaults.mcc }}'
mnc: '{{ ueransim_topology.defaults.mnc }}'
nci: '{{ cell.gnb.nci }}'
idLength: 32
tac: {{ cell.gnb.tac }}
linkIp: 0.0.0.0
ngapIp: 0.0.0.0
gtpIp: 0.0.0.0
gtpPort: 2152
ngapPort: 38412
amfConfigs:
  # Per-cell AMF IP: {cell_n2_subnet_prefix}.{cell_id}.{amf_cell_n2_ip_suffix}
  # gNB connects to AMF via per-cell N2 overlay (same subnet as gNB's n2-cell-X interface)
  - address: {{ cell_n2_subnet_prefix }}.{{ cell.id }}.{{ amf_cell_n2_ip_suffix }}
    port: 38412
slices:
{% for slice in cell.gnb.slices | default([{ 'sst': 1, 'sd': 1 }]) %}
  - sst: {{ slice.sst }}
    sd: {{ slice.sd }}
{% endfor %}
ignoreStreamIds: true

